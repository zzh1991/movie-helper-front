{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    ["@babel/preset-react", {
      "modules": false
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    ["import", {
      "libraryName": "antd",
      "style": true,
      "libraryDirectory": "es"
    }],
    ["@babel/plugin-proposal-class-properties"]
  ]
}
